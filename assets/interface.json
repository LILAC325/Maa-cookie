{
    "name":"Maa-cookiek",
    "url":"https://github.com/LILAC325/Maa-cookie",
    "message":"welecome to cookiekingdom",
    "version":"v1.0.0",
    "controller": [
        {
            "name": "ADB默认",
            "type": "Adb"
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py",
            "{SOCKET_ID}"
        ]
    },
    "resource": [
        {
            "name": "Official",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "task": [
        
        {
            "name":"清绿点",
            "entry":"cleargreenpoint"
        },
        {
            "name":"王国生产管理",
            "entry":"kingdomproduce"
        },
        {
            "name":"每日奖励",
            "entry":"everydayfreereward"
        },
        {
            "name":"进度奖励",
            "entry":"progressreward"
        },
        {
            "name":"商城",
            "entry":"freeshopping"
        },
        {
            "name":"交易所",
            "entry":"exchange"
        },
        {
            "name":"王国活动",
            "entry":"kingdomactivity",
            "option":[
                "愿望树",
                "熊熊果冻热气球",
                "海上贸易中心",
                "熊熊果冻列车",
                "交易所",
                "相约之所"
            ]
        }
    ],
    "option":{
        "愿望树":{
            "cases":[
                {
                    "name":"启用",
                    "pipeline_override":{
                        "choose_wishingtree":{
                            "enabled":true
                        }
                    }
                },
                {
                    "name":"禁用",
                    "pipeline_override":{
                        "choose_wishingtree":{
                            "enabled":false
                        }
                    }
                }
                
            ]
        },
        "熊熊果冻热气球":{
            "cases":[
                {
                    "name":"启用",
                    "pipeline_override":{
                        "choose_hotairballoon":{
                            "enabled":true
                        }
                    }
                },
                {
                    "name":"禁用",
                    "pipeline_override":{
                        "choose_hotairballoon":{
                            "enabled":false
                        }
                    }
                }
            ]
        },
        "海上贸易中心":{
            "cases":[
                {
                    "name":"启用",
                    "pipeline_override":{
                        "choose_trade":{
                            "enabled":true
                        }
                    }
                },
                {
                    "name":"禁用",
                    "pipeline_override":{
                        "choose_trade":{
                            "enabled":false
                        }
                    }
                }
            ]
        },
        "熊熊果冻列车":{
            "cases":[
                {
                    "name":"启用",
                    "pipeline_override":{
                        "choose_train":{
                            "enabled":true
                        }
                    }
                },
                {
                    "name":"禁用",
                    "pipeline_override":{
                        "choose_train":{
                            "enabled":false
                        }
                    }
                }
            ]
        },
        "交易所":{
            "cases":[
                {
                    "name":"启用",
                    "pipeline_override":{
                        "choose_exchange":{
                            "enabled":true
                        }
                    }
                },
                {
                    "name":"禁用",
                    "pipeline_override":{
                        "choose_exchange":{
                            "enabled":false
                        }
                    }
                }
            ]
        },
        "相约之所":{
            "cases":[
                {
                    "name":"启用",
                    "pipeline_override":{
                        "choose_specialcookie":{
                            "enabled":true
                        }
                    }
                },
                {
                    "name":"禁用",
                    "pipeline_override":{
                        "choose_specialcookie":{
                            "enabled":false
                        }
                    }
                }
            ]
        }
    }
}
