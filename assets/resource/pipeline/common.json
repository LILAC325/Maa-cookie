{
    "returnmainpage":{
        "next": [
            "[JumpBack]check_over_quit",
            "[JumpBack]chcek_over_back",
            "mainpagejudgment",
            "[JumpBack]back",
            "[JumpBack]quit",
            "[JumpBack]wait_mainpagejudgment"
            
        ],
        "post_delay":2000
    },
    "mainpagejudgment":{
        "recognition":{
            "type":"TemplateMatch",
            "param":{
                "template":"common/mainpage.png",
                "recommended roi" : [37,550,167,60]
            }
        }
    },
    "wait_mainpagejudgment":{
        "recognition":{
            "type":"TemplateMatch",
            "param":{
                "template":"common/mainpage.png",
                "recommended roi" : [37,550,167,60]
            }
        },
        "inverse":true,
        "post_delay":1000
    },
    "quit":{
        "recognition":{
            "type":"TemplateMatch",
            "param":{
                "template" : "common/return.png",
                "roi" : [1137,2,142,114]
            }
        },
        "action":{
            "type":"Click"
        },
        "post_delay":2000
    },
    "check_over_quit":{
        "recognition":{
            "type":"OCR",
            "param":{
                "expected":[
                    "光荣殿堂"
                ],
                "roi" : [1008,90,94,36],
                "only_rec":true
            }
        },
        "next":[
            "back"
        ],
        "post_delay":2000
    },
    "chcek_over_back":{
        "recognition":{
            "type":"OCR",
            "param":{
                "expected":[
                    "要结束游戏吗"
                ],
                "roi" : [557,363,158,32],
                "only_rec":true
            }
        },
        "next":[
            "back"
        ],
        "post_delay":2000
    },
    "bottom_click":{
        "recognition":{
            "type":"OCR",
            "param":{
                "expected":[
                    "点击画面继续",
                    "点击完成",
                    "点击以完成",
                    "点击以继续"
                ]
            }
        },
        "action":{
            "type":"Click"
        },
        "post_delay":2000
    },
    "comfirm":{
        "recognition":{
            "type":"OCR",
            "param":{
                "expected":[
                    "确定",
                    "确认"
                ]
            }
        },
        "action":{
            "type":"Click"
        }
    },
    "back":{
        "action":{
            "type":"ClickKey",
            "param":{
                "key" : [
                    111
                ]
            }
        },
        "pre_delay":2000
    },
    "greenpoint_click":{
        "recognition":{
            "type":"TemplateMatch",
            "param":{
                "template":[
                    "common/greenpoint.png",
                    "common/greenpoint1.png"
                ]
            }
        },
        "action":{
            "type":"Click"
        }
    },
    "exit_task":{
        "action":{
            "type":"StopTask"
        }
    }
}
