{
    "startup":{
        "post_wait_freeze":300,
        "timeout":60000,
        "next":[
            "[JumpBack]start_game",
            "[JumpBack]login_download",
            "[JumpBack]quit",
            "[JumpBack]mainpagejudgment"
        ]
    },
    "start_game":{
        "action":{
            "type":"StartApp",
            "param":{
                "package":"com.tencent.cookie"
            }
        },
        "next":[
            "start_game_done"
        ],
        "post_delay":30000
    },
    "start_game_done":{
        "action":{
            "type":"Custom",
            "param":{
                "custom_action": "DisableNode",
                "custom_action_param": {
                    "node_name": "start_game"
                }
            }
        }
    },
    "login_download":{
        "recognition":{
            "type":"OCR",
            "param":{
                "expected":[
                    "有可下载的数据"
                ],
                "roi" : [351,169,584,384]
            }
        },
        "next":[
            "exit_task"
        ]
    },
    "login_page":{
        "recognition":{
            "type":"OCR",
            "param":{
                "expected" : [
                    "点击开始"
                ],
                "roi" : [537,632,188,53]
            }
        },
        "action":{
            "type":"Click"
        },
        "next":[
            "[JumpBack]returnmainpage"
        ],
        "post_delay":3000
    }
}